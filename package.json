{
  "name": "initiative-tool",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "dexie": "^2.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.map": "^4.6.0",
    "node-sass": "^4.8.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "sass-lint": "^1.12.1",
    "storybook": "^1.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && node unhash.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-css": "yarn sass-lint src/**/*.scss -v -q && yarn node-sass src/ -o src/",
    "watch-css": "yarn sass-lint src/**/*.scss -v -q && yarn build-css && yarn node-sass src/ -o src --watch --recursive"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15"
  }
}
